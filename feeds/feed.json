---
layout: null
---
{% assign first = true %}
[{% for post in site.posts %}
{% if post.title != null and post.title != empty and post.date != null %}
{% unless first %},{% endunless %}{
    "title": {{post.title | jsonify}},
    "description": {{post.description | markdownify | strip_html | jsonify}},
    "content": {{post.content | markdownify | strip_html | jsonify}},
    "link": "{{ site.baseurl }}{{ post.url }}",
    "date": "{{ post.date }}",
    "excerpt": "{{ post.snippet }}",
    "search_omit": "{{ post.search_omit }}"
    "image": "{{ post.image }}",
}
{% assign first = false %}
{% endif %}
{% endfor %}]